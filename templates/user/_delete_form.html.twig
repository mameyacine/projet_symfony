<div class="w-full flex justify-between">
    <form method="post" action="{{ path('delete_user', {'idA': admin_id, 'id': user.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
        <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ user.id) }}">
        <button class="logout"><i class="fas fa-trash"></i></button>
    </form>
</div>