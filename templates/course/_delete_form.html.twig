
    <form method="post" action="{{ path('delete_course', { 'idA': admin_id, 'id': course.id }) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
        <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ course.id) }}">
        <button type="submit" class="m-2 logout">
            <i class="fas fa-trash"></i> 
        </button>
    </form>



